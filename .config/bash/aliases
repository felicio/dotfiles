alias ln="ln -iv"
alias ls="ls -FGh"
alias ll="ls -l"
alias lla="ls -lA"
alias egrep="egrep --colour=auto"
#alias ssh="ssh -q"
alias scp="scp -q"
alias zip="zip -db"
alias link="link -i"
alias pwd="pwd -P"
alias bc="bc -ql"
alias ps="ps -hc"
alias pgrep="pgrep -lf"
alias pkill="pkill -Il"
alias less="less -R"
alias disk="df -PH | egrep disk"
alias memory="top -stats pid,mem,command -n 22 -o mem"
alias cpu="top -stats pid,cpu,command -n 22 -o cpu"
alias ec2s='aws ec2 describe-instances --filters "Name=instance-state-name,Values=running"'
alias dclean="docker rmi $(docker images | awk '/^<none>/ {print $3}')"
alias mc="mc --nocolor"
alias grep="grep --color=always"
alias ping="ping -oq"
alias chrome="/Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome"
alias chrome-debug="nohup /Applications/Google\ Chrome.app/Contents/MacOS/Google\ Chrome --remote-debugging-port=9222 --user-data-dir=/tmp/chrome-debug about:blank > /dev/null &"
alias canary="/Applications/Google\ Chrome\ Canary.app/Contents/MacOS/Google\ Chrome\ Canary"
alias canary-debug="nohup /Applications/Google\ Chrome\ Canary.app/Contents/MacOS/Google\ Chrome\ Canary --user-data-dir=/tmp/canary-debug --remote-debugging-port=9222 --auto-open-devtools-for-tabs about:blank > /dev/null &"
alias chromium="/Applications/Chromium.app/Contents/MacOS/Chromium"
alias dot='/usr/bin/git --git-dir="$HOME"/dotfiles.git --work-tree="$HOME"'
alias rm="rm -i"

